{
  "name": "vscode-jupyter-notebook-keymap",
  "displayName": "Jupyter Keymap for (all) Notebooks",
  "description": "",
  "version": "0.0.1",
  "enableProposedApi": true,
  "engines": {
    "vscode": "1.57.0-insider"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onNotebook:*"
  ],
  "main": "./out/extension.js",
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Notebook",
        "command": "notebook.cell.changeToHeading1",
        "title": "Change Cell To Heading 1",
        "enablement": "notebookCellListFocused"
      },
      {
        "category": "Notebook",
        "command": "notebook.cell.changeToHeading2",
        "title": "Change Cell To Heading 2",
        "enablement": "notebookCellListFocused"
      },
      {
        "category": "Notebook",
        "command": "notebook.cell.changeToHeading3",
        "title": "Change Cell To Heading 3",
        "enablement": "notebookCellListFocused"
      },
      {
        "category": "Notebook",
        "command": "notebook.cell.changeToHeading4",
        "title": "Change Cell To Heading 4",
        "enablement": "notebookCellListFocused"
      },
      {
        "category": "Notebook",
        "command": "notebook.cell.changeToHeading5",
        "title": "Change Cell To Heading 5",
        "enablement": "notebookCellListFocused"
      },
      {
        "category": "Notebook",
        "command": "notebook.cell.changeToHeading6",
        "title": "Change Cell To Heading 6",
        "enablement": "notebookCellListFocused"
      }
    ],
    "keybindings": [
      {
        "mac": "1",
        "win": "1",
        "linux": "1",
        "key": "1",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.changeToHeading1"
      },
      {
        "mac": "2",
        "win": "2",
        "linux": "2",
        "key": "2",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.changeToHeading2"
      },
      {
        "mac": "3",
        "win": "3",
        "linux": "3",
        "key": "3",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.changeToHeading3"
      },
      {
        "mac": "4",
        "win": "4",
        "linux": "4",
        "key": "4",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.changeToHeading4"
      },
      {
        "mac": "5",
        "win": "5",
        "linux": "5",
        "key": "5",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.changeToHeading5"
      },
      {
        "mac": "6",
        "win": "6",
        "linux": "6",
        "key": "6",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.changeToHeading6"
      },
      {
        "mac": "F",
        "win": "F",
        "linux": "F",
        "key": "F",
        "when": "notebookCellListFocused",
        "command": "notebook.find"
      },
      {
        "mac": "K",
        "win": "K",
        "linux": "K",
        "key": "K",
        "when": "notebookCellListFocused",
        "command": "list.focusUp"
      },
      {
        "mac": "J",
        "win": "J",
        "linux": "J",
        "key": "J",
        "when": "notebookCellListFocused",
        "command": "list.focusDown"
      },
      {
        "mac": "A",
        "win": "A",
        "linux": "A",
        "key": "A",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.insertCodeCellAbove"
      },
      {
        "mac": "B",
        "win": "B",
        "linux": "B",
        "key": "B",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.insertCodeCellBelow"
      },
      {
        "mac": "D D",
        "win": "D D",
        "linux": "D D",
        "key": "D D",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.delete"
      },
      {
        "mac": "Z",
        "win": "Z",
        "linux": "Z",
        "key": "Z",
        "when": "notebookCellListFocused",
        "command": "jupyter.notebookeditor.keybind.undo"
      },
      {
        "mac": "S",
        "win": "S",
        "linux": "S",
        "key": "S",
        "when": "notebookCellListFocused",
        "command": "jupyter.notebookeditor.keybind.save"
      },
      {
        "mac": "C",
        "win": "C",
        "linux": "C",
        "key": "C",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.copy"
      },
      {
        "mac": "X",
        "win": "X",
        "linux": "X",
        "key": "X",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.cut"
      },
      {
        "mac": "V",
        "win": "V",
        "linux": "V",
        "key": "V",
        "when": "notebookCellListFocused",
        "command": "notebook.cell.paste"
      },
      {
        "mac": "O",
        "win": "O",
        "linux": "O",
        "key": "O",
        "when": "notebookCellListFocused",
        "command": "jupyter.notebookeditor.keybind.toggleOutput"
      },
      {
        "mac": "I I",
        "win": "I I",
        "linux": "I I",
        "key": "I I",
        "when": "notebookCellListFocused",
        "command": "jupyter.notebookeditor.interruptkernel"
      },
      {
        "mac": "O O",
        "win": "O O",
        "linux": "O O",
        "key": "O O",
        "when": "notebookCellListFocused",
        "command": "jupyter.notebookeditor.restartkernel"
      },
      {
        "mac": "shift+l",
        "win": "shift+l",
        "linux": "shift+l",
        "key": "shift+l",
        "when": "notebookCellListFocused",
        "command": "notebook.toggleLineNumbers"
      },
      {
        "mac": "ctrl+shift+-",
        "win": "ctrl+shift+-",
        "linux": "ctrl+shift+-",
        "when": "editorTextFocus && inputFocus && notebookEditorFocused",
        "command": "notebook.cell.split"
      },
      {
        "key": "ctrl+enter",
        "when": "editorTextFocus && inputFocus && notebookEditorFocused",
        "command": "notebook.cell.execute"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "download-api": "vscode-dts dev -f",
    "postdownload-api": "vscode-dts main -f",
    "postinstall": "npm run download-api"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-dts": "^0.3.1",
    "vscode-test": "^1.5.0"
  }
}
